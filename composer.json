{
    "name": "tommyys/laravel_library",
    "description": "Default Laravel library for Axstarzy Dot Com that includes the most used libraries, plugins, helper classes, and migrations",
    "authors": [
        {
            "name": "Tommy Lee",
            "email": "tommy.lee@axstarzy.com"
        }
    ],
    "minimum-stability": "dev",
    "require": {
        "php": "^7.0",
        "guzzlehttp/guzzle": "^6.2",
        "intervention/image": "^2.4",
        "laravelcollective/html": ">=5.1.0",
        "funkjedi/composer-include-files": "^1.0"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Axstarzy\\LaravelLibrary\\LaravelLibraryServiceProvider"
            ]
        }
    },
    "autoload": {
        "classmap": [
        ],
        "psr-4": {
            "Axstarzy\\LaravelLibrary\\": "src/"
        }
    }
}
